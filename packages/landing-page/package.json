{
    "name": "landing-page",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "cross-env ENVIRONMENT=$ENVIRONMENT run-p type-check \"build-only {@}\" -- --mode ${ENVIRONMENT:-development}",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint --ext .tsx,.ts,.js,.vue ./ && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js,.vue ./ && prettier --write './**/*.{json,md}'"
    },
    "dependencies": {
        "@fontsource-variable/inter": "^5.0.18",
        "@metrom-xyz/contracts": "^0.11.0",
        "@metrom-xyz/ui": "workspace:*",
        "vue": "^3.4.30",
        "vue-i18n": "^9.13.1",
        "vue3-marquee": "^4.2.0"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.14.1",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.18",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.26.0",
        "npm-run-all2": "^6.2.0",
        "postcss": "^8.4.35",
        "prettier": "^3.3.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.0",
        "vite": "^5.2.12",
        "vue-tsc": "^2.0.19"
    }
}
