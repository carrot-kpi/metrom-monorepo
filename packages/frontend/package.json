        "@wagmi/core": "^2.6.5",
        "dayjs": "^1.11.10",
        "graphql": "^16.8.1",
        "graphql-request": "^6.1.0",
        "pinia": "^2.1.7",
        "vevm": "^0.1.1",
        "viem": "^2.8.5",
        "vue": "^3.4.21",
        "vue-i18n": "9",
        "vue-imask": "^7.5.0",
