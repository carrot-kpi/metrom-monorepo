{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "cross-env ENVIRONMENT=$ENVIRONMENT run-p type-check \"build-only {@}\" -- --mode ${ENVIRONMENT:-development}",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint --ext .tsx,.ts,.js,.vue ./ && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js,.vue ./ && prettier --write './**/*.{json,md}'"
    },
    "dependencies": {
        "@floating-ui/vue": "^1.0.6",
        "@fontsource/inter": "^5.0.18",
        "@metrom-xyz/contracts": "^0.9.0",
        "@uniswap/token-lists": "1.0.0-beta.34",
        "@vueuse/core": "^10.10.0",
        "@wagmi/connectors": "^5.0.7",
        "@wagmi/core": "^2.10.5",
        "dayjs": "^1.11.10",
        "decimal.js-light": "^2.5.1",
        "graphql": "^16.8.1",
        "graphql-request": "^6.1.0",
        "jwt-decode": "^4.0.0",
        "pinia": "^2.1.7",
        "sdk": "workspace:*",
        "uuid": "^9.0.1",
        "vevm": "^0.1.1",
        "viem": "^2.13.5",
        "vue": "^3.4.27",
        "vue-i18n": "^9.13.1",
        "vue-imask": "^7.6.1",
        "vue-router": "^4.3.2"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.10.3",
        "@tsconfig/node20": "^20.1.4",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.14.1",
        "@types/uuid": "^9.0.8",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.19",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.26.0",
        "jsdom": "^24.1.0",
        "npm-run-all2": "^6.2.0",
        "postcss": "^8.4.38",
        "prettier": "^3.3.0",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.0",
        "vite": "^5.2.12",
        "vitest": "^1.6.0",
        "vue-tsc": "^2.0.19"
    }
}
