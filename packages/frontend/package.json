        "@uniswap/token-lists": "1.0.0-beta.34",
        "@vueuse/core": "^10.11.0",
        "@wagmi/connectors": "^5.0.21",
        "@wagmi/core": "^2.11.6",
        "dayjs": "^1.11.10",
        "decimal.js-light": "^2.5.1",
        "ethereum-blockies-base64": "^1.0.2",
        "graphql": "^16.9.0",
        "graphql-request": "^7.1.0",
        "jwt-decode": "^4.0.0",
        "pinia": "^2.1.7",
        "sdk": "workspace:*",
        "uuid": "^10.0.0",
        "vevm": "^0.2.0",
        "viem": "^2.16.5",
        "vue": "^3.4.31",
        "vue-collapsed": "^1.3.3",
