{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:dev": "run-p type-check \"build-only {@}\" -- --mode development",
        "build:staging": "run-p type-check \"build-only {@}\" -- --mode staging",
        "build:prod": "run-p type-check \"build-only {@}\" -- --mode production",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint --ext .tsx,.ts,.js,.vue ./ && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js,.vue ./ && prettier --write './**/*.{json,md}'"
    },
    "dependencies": {
        "@floating-ui/vue": "^1.0.6",
        "@fontsource/inter": "^5.0.18",
        "@metrom-xyz/contracts": "^0.7.0",
        "@uniswap/token-lists": "1.0.0-beta.34",
        "@vueuse/core": "^10.9.0",
        "@wagmi/connectors": "^4.1.28",
        "@wagmi/core": "^2.6.19",
        "dayjs": "^1.11.10",
        "decimal.js-light": "^2.5.1",
        "graphql": "^16.8.1",
        "graphql-request": "^6.1.0",
        "jwt-decode": "^4.0.0",
        "pinia": "^2.1.7",
        "sdk": "workspace:*",
        "uuid": "^9.0.1",
        "vevm": "^0.1.1",
        "viem": "^2.9.26",
        "vue": "^3.4.25",
        "vue-i18n": "^9.13.1",
        "vue-imask": "^7.6.0",
        "vue-router": "^4.3.2"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.10.2",
        "@tsconfig/node20": "^20.1.4",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.12.7",
        "@types/uuid": "^9.0.8",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.25.0",
        "jsdom": "^24.0.0",
        "npm-run-all2": "^6.1.2",
        "postcss": "^8.4.38",
        "prettier": "^3.0.3",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.0",
        "vite": "^5.2.10",
        "vitest": "^1.5.2",
        "vue-tsc": "^2.0.14"
    }
}
