{
    "type": "module",
    "private": true,
    "name": "algebra-integral-subgraph",
    "scripts": {
        "update-addresses": "tsx ./scripts/update-addresses.ts",
        "codegen": "graph codegen",
        "build:only": "graph build",
        "build": "pnpm update-addresses placeholder placeholder && pnpm codegen && graph build",
        "deploy:test-integral:holesky": "pnpm update-addresses holesky test-integral && graph codegen && graph deploy --studio --network holesky metrom-test-integral-holesky",
        "lint": "eslint --ext .js,.ts ./ && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .js,.ts ./ && prettier --write './**/*.{json,md}'"
    },
    "dependencies": {
        "@graphprotocol/graph-cli": "0.69.1",
        "@graphprotocol/graph-ts": "0.34.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-turbo": "^1.13.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "tsx": "^4.7.1"
    }
}
